

import java.util.Scanner;

public class MonkattheGraphFactory {
	public static void main(String argv[]){
		Scanner asd=new Scanner(System.in);
		int sum = 0;
		int n=asd.nextInt();
		for(int i=0;i<n;i++){
			sum+=asd.nextInt();
		}
		if((n-1)*2==sum)
			System.out.println("Yes");
		else
			System.out.println("No");
	}
}
